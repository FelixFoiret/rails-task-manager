<%= link_to 'Modify your task', edit_task_path %>
<%= link_to 'Back to Tasks List', tasks_path %>
<%= link_to "Delete #{@task.title}",
            task_path(@task),
            method: :delete %>

<h1><%= @task.title  %></h1>
<p> <%= @task.details %></p>
<%= check_box_tag(@task.title,'yes',@task.completed) %>
